{
  "manifest_version": 2,

  "name": "Use the Horse",
  "description": "This extension replaces the word 'Force' with 'Horse'",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["replace.js"]
    }
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}